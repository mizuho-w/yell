<div class="row">
	<div class="bg-white radius genre-index pd-20">
		<%= render 'genres/genre' %>
	</div>

	<div class="items">
		<% @items.each do |i| %>
		<div class="items_view">
			<%= link_to item_path(i) do %>
			<%=attachment_image_tag(i,:img ,:fill,300,300,fallback:"noimg.jpg",size:"200x200") %>
		    <% end %>
		</div>
		<% end %>
	</div>
</div>
<div class="col-md-offset-10">
	<%= paginate @items %>
</div>
